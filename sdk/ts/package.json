{
  "name": "@stratusdb/sdk",
  "version": "0.1.0",
  "description": "Zero-ORM TypeScript SDK for PostgreSQL with connection pooling and type-safe query execution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./pool": {
      "types": "./dist/pool.d.ts",
      "require": "./dist/pool.js",
      "import": "./dist/pool.mjs"
    },
    "./executor": {
      "types": "./dist/executor.d.ts",
      "require": "./dist/executor.js",
      "import": "./dist/executor.mjs"
    },
    "./transaction": {
      "types": "./dist/transaction.d.ts",
      "require": "./dist/transaction.js",
      "import": "./dist/transaction.mjs"
    },
    "./params": {
      "types": "./dist/params.d.ts",
      "require": "./dist/params.js",
      "import": "./dist/params.mjs"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.module.json",
    "build:watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "postgresql",
    "database",
    "sdk",
    "orm",
    "type-safe",
    "stratus"
  ],
  "author": "Stratus Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratusdb/stratus"
  },
  "peerDependencies": {
    "pg": ">=8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
